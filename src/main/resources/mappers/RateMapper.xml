        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.trifling.things.repository.RateMapper">

    <insert id="rateWrite">
        INSERT INTO m_rate
            (user_num, movie_num, rate_review, rate_score)
